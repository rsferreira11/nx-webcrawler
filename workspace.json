{
  "version": 2,
  "projects": {
    "faas-to-number": {
      "root": "libs/faas/to-number",
      "sourceRoot": "libs/faas/to-number/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/faas/to-number/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/faas/to-number"],
          "options": {
            "jestConfig": "libs/faas/to-number/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-puller": {
      "root": "apps/web-puller",
      "sourceRoot": "apps/web-puller/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/web-puller",
            "main": "apps/web-puller/src/main.ts",
            "tsConfig": "apps/web-puller/tsconfig.app.json",
            "assets": ["apps/web-puller/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/web-puller/src/environments/environment.ts",
                  "with": "apps/web-puller/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "web-puller:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/web-puller/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/web-puller"],
          "options": {
            "jestConfig": "apps/web-puller/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "web-puller"
}
