{
  "version": 2,
  "projects": {
    "faas-to-number": {
      "root": "libs/faas/to-number",
      "sourceRoot": "libs/faas/to-number/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/faas/to-number/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/faas/to-number"],
          "options": {
            "jestConfig": "libs/faas/to-number/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-kafka": {
      "root": "libs/shared/kafka",
      "sourceRoot": "libs/shared/kafka/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/kafka/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/kafka"],
          "options": {
            "jestConfig": "libs/shared/kafka/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
